#include<stdio.h>
void main()
{
	 int i,k,sum=0,missing;
	 printf("enter the size of an array : ");
	 scanf("%d",&k);
	 int arr[k-1];
	 printf("enter the elements\n");
	 for(i=0;i<k-1;i++)
	 {
	     scanf("%d",&arr[i]);
	     sum=sum+arr[i];
	 }
	 missing = (k*(k+1))/2 - sum;
	 printf("missing number is = %d", missing);
	 return 0;
}
